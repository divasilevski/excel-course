{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["warn", 2, { "MemberExpression": 1, "SwitchCase": 1 }],
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "no-undef": "off",
    "operator-linebreak": "off",
    "space-before-function-paren": "off"
  },
  "extends": ["eslint:recommended", "google"]
}